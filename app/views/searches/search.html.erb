<h2>Search Results</h2>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% if @range == "User" %>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.name, user_path(user) %></td>
        </tr>
      <% end %>
    <% else %>
      <% @stretch_programs.each do |stretch_program| %>
        <tr>
          <td><%= link_to stretch_program.title, stretch_program_path(stretch_program) %></td>
          <td><%= stretch_program.description %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>